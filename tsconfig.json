{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "baseUrl": ".",
    "allowJs": true,
    "checkJs": true,
    "paths": {
      "@fmgy/*": ["./src/*"]
    },
    "types": ["unplugin-icons/types/vue", "vue"],
    "jsx": "preserve"
  },
  "exclude": ["node_modules"],
  "include": ["src/**/*.d.ts", "src/**/*.ts", "src/**/*.js", "src/**/*.vue"]
}
